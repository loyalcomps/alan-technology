<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record id="ticket_number" model="ir.sequence">
            <field name="name">Service Ticket Number</field>
            <field name="code">ticket.number</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SR#</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
     <data noupdate="1">
        <record id="request_number" model="ir.sequence">
            <field name="name">Service Request Number</field>
            <field name="code">request.number</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>