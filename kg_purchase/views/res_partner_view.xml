<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="res_partner_inherited_view_id">
        <field name="name">res.partner</field>
        <field name="model">res.partner</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="supplier_rank" invisible="1"/>
                <field name="kg_supplier_type_id" attrs="{'invisible': [('supplier_rank','=',0)]}"/>
            </xpath>

            <xpath expr="//field[@name='team_id']" position="after">
                <field name="customer"/>
            </xpath>

            <xpath expr="//field[@name='property_payment_term_id']" position="after">
                <field name="notify_email"/>
            </xpath>
            <xpath expr="//field[@name='property_supplier_payment_term_id']" position="before">
                <field name="vendor"/>
            </xpath>
        </field>
    </record>
</odoo>